<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.2" xmlns:https="http://www.mulesource.org/schema/mule/https/2.2"
	xmlns:http="http://www.mulesource.org/schema/mule/http/2.2"
	xsi:schemaLocation="
               http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
               http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
               http://www.mulesource.org/schema/mule/https/2.2 http://www.mulesource.org/schema/mule/https/2.2/mule-https.xsd
               http://www.mulesource.org/schema/mule/cxf/2.2  http://www.mulesource.org/schema/mule/cxf/2.2/mule-cxf.xsd
               http://www.mulesource.org/schema/mule/http/2.2 http://www.mulesource.org/schema/mule/http/2.2/mule-http.xsd">

	<https:endpoint name="outboundEndpoint" synchronous="true"
		address="${outbound.endpoint.dominohttpadapter}" >
		<message-properties-transformer>
			<add-message-property key="http.version" value="${http.version}" />
		</message-properties-transformer>
	</https:endpoint>
	
	<model name="AssertCareEngagement-model">
		<service name="testComponent">
			<inbound>
				<http:inbound-endpoint address="${inbound.endpoint.dominohttpadapter}" synchronous="true" />
			</inbound>
			<outbound>
				<pass-through-router>
					<https:outbound-endpoint ref="outboundEndpoint" synchronous="true"/>
				</pass-through-router>
			</outbound>
		</service>
	</model>
</mule>
